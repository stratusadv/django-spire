from __future__ import annotations

from dandy.llm import BaseLlmBot

from ${module_path}.intelligence import intel
from ${module_path}.intelligence import prompts


class ${bot_class_name}(BaseLlmBot):
    instructions_prompt = prompts.${instruction_prompt_function_name}()
    intel_class = intel.${intel_class_name}

    @classmethod
    def process(
        cls,
        user_input: str
    ) -> intel.${intel_class_name}:

        return cls.process_prompt_to_intel(
            prompt=prompts.${user_input_prompt_function_name}(user_input)
        )
